<%= simple_form_for(@event) do |f| %>
  <%= f.error_notification %>
  <%= f.input :name %>
  <%= f.input :date, html5: true %>
  <%= f.input :description %>
  <%= f.association :promotions, as: :check_boxes %>
  <%= f.association :teachers, as: :check_boxes, collection: Student.admin %>
  <%= f.button :submit %>
<% end %>
