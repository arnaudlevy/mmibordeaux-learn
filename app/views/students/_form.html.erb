<%= simple_form_for @student do |f| %>
  <%= f.error_notification %>
  <%= f.input :firstname %>
  <%= f.input :lastname %>
  <%= f.association :promotion %>
  <%= f.input :codeschool %>
  <%= f.input :codecademy %>
  <%= f.input :github_identifier %>
  <%= f.input :github_repository %>
  <%= f.input :heroku_app %>
  <%= f.button :submit, 'Enregistrer' %>
<% end %>
